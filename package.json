{
  "name": "jest-parallel-worker",
  "version": "1.0.0",
  "description": "A Jest wrapper for parallel test execution at test level",
  "main": "src/index.js",
  "bin": {
    "jest-parallel": "./bin/jest-parallel.js"
  },
  "scripts": {
    "test": "node bin/jest-parallel.js --testMatch 'tests/**/*.test.js' --mode native-parallel",
    "test:examples": "node bin/jest-parallel.js --testMatch 'examples/**/*.test.js' --mode parallel-test",
    "test:file-level": "node bin/jest-parallel.js --testMatch 'examples/**/*.test.js' --mode parallel-file --forceConcurrent",
    "test:jest-parallel": "node bin/jest-parallel.js --testMatch 'examples/**/*.test.js' --mode jest-parallel --forceConcurrent",
    "test:native-parallel": "node bin/jest-parallel.js --testMatch 'examples/**/*.test.js' --mode native-parallel"
  },
  "keywords": [
    "jest",
    "parallel",
    "testing",
    "worker",
    "performance"
  ],
  "author": "Jest Parallel Worker",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "expect": "^30.0.5",
    "glob": "^10.3.3",
    "jest": "^29.6.2"
  },
  "devDependencies": {
    "jest": "^29.6.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
