{
  "name": "jest-parallel-worker",
  "version": "1.0.0",
  "description": "A Jest SDK for parallel test execution with enhanced reporting and performance optimization",
  "main": "src/index.js",
  "bin": {
    "jest-parallel": "./bin/jest-parallel.js"
  },
  "files": [
    "src/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node bin/jest-parallel.js --testMatch 'tests/**/*.test.js' --mode native-parallel",
    "test:sdk": "node -e \"const sdk = require('./src'); console.log('SDK loaded successfully:', Object.keys(sdk));\"",
    "prepublishOnly": "npm run test:sdk"
  },
  "keywords": [
    "jest",
    "parallel",
    "testing",
    "sdk",
    "worker",
    "performance",
    "test-runner",
    "automation"
  ],
  "author": "Jest Parallel Worker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bsautomation/jest-parallel-worker.git"
  },
  "bugs": {
    "url": "https://github.com/bsautomation/jest-parallel-worker/issues"
  },
  "homepage": "https://github.com/bsautomation/jest-parallel-worker#readme",
  "dependencies": {
    "browserstack-node-sdk": "1.36.6",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "expect": "^30.0.5",
    "glob": "^10.3.3"
  },
  "peerDependencies": {
    "jest": ">=29.0.0",
    "@playwright/test": "^1.54.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
